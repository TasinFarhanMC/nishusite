{
  "name": "nishusite",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all -p watch:proto astro:dev",
    "astro:dev": "astro dev",
    "build": "npm run gen:proto && astro build",
    "preview": "npm run gen:proto && astro build && wrangler dev",
    "astro": "astro",
    "deploy": "npm run gen:proto && astro build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "gen:proto": "mkdir -p src/assets/gen && protoc -I=src/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/assets/gen --ts_proto_opt=esModuleInterop=true,outputServices=grpc-js,outputJsonMethods=true src/protos/*.proto",
    "watch:proto": "chokidar 'src/protos/*.proto' -c 'bun run gen:proto --file {path}'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.3",
    "@astrojs/svelte": "^7.1.0",
    "astro": "^5.12.9",
    "ts-proto": "^2.7.7"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.33.0",
    "@eslint/markdown": "^7.1.0",
    "@types/node": "^24.2.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.39.1",
    "wrangler": "^4.28.1"
  }
}
